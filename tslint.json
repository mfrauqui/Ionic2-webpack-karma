{
  "rules": {
    "class-name":true,
    "no-duplicate-variable": true,
    "no-unused-variable": [
      true
    ],
      
    "no-eval":true,
    "no-internal-module": true,
    "no-var-keyword": true,
    "one-line": [
        true,
        "check-open-brace",
        "check-whitespace"
    ],
     
     "variable-name": [
        true,
        "ban-keywords",
        "check-format"
    ] 
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ]
}
